if ("<%= highlight %>" == "true") {
    $(".collapsible").html("<%= escape_javascript(render(:partial => 'task', locals: {answer: @answer, isHighlight: true}))%>");
    instance.open(0);
}
else {
    $(".collapsible").html("<%= escape_javascript(render(:partial => 'task', locals: {answer: @answer}))%>");
    swal("Thank you", "Successfully saved!", "success");
    instance.open(0);
}